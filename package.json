{
  "name": "official-site",
  "version": "1.0.0",
  "author": "fegos",
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-import": "^1.2.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "marked": "^0.3.12",
    "null-loader": "^0.1.1",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "react-markdown": "^3.1.5",
    "server-style-loader": "^1.0.0",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "cross-env": "^5.1.1",
    "esky-mock": "~1.0.7",
    "babel-eslint": "8.0.2",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.0",
    "mocha": "^3.4.2",
    "node-gyp": "^3.6.2",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "^2.5.0"
  },
  "scripts": {
    "init": "npm install --no-optional --no-package-lock --registry=https://registry.npm.taobao.org && npm run init-desktop && npm run init-mobile && npm run init-server",
    "init-prod": "npm install --prefer-offline --production --no-optional --registry=https://registry.npm.taobao.org && npm run init-prod-desktop && npm run init-prod-mobile && npm run init-prod-server",
    "init-mobile": "cd client/mobile && npm run init",
    "init-desktop": "cd client/desktop && npm run init",
    "init-server": "cd server && npm run init",
    "init-prod-mobile": "cd client/mobile && npm run init-prod",
    "init-prod-desktop": "cd client/desktop && npm run init-prod",
    "init-prod-server": "cd server && npm run init-prod",
    "start-desktop": "cross-env NODE_ENV=development BABEL_ENV=desktop webpack-dev-server --progress --profile --colors --config config/webpack.dev.desktop.config.js",
    "start-mobile": "cross-env NODE_ENV=development BABEL_ENV=mobile webpack-dev-server --progress --profile --colors --config config/webpack.dev.mobile.config.js",
    "build-desktop": "NODE_ENV=production BABEL_ENV=desktop webpack --profile --colors --config config/webpack.prod.desktop.config.js",
    "build-mobile": "NODE_ENV=production BABEL_ENV=mobile webpack --profile --colors --config config/webpack.prod.mobile.config.js",
    "build": "npm run clean && npm run build-desktop && npm run build-mobile",
    "release": "npm run set-loglevel && npm run init-prod && npm run build",
    "set-loglevel": "npm config set loglevel error",
    "server": "cd server && npm run start",
    "stop": "cd server && npm run stop",
    "clean": "rm -rf ./build ./server/client",
    "remove": "rm -rf node_modules server/node_modules client/desktop/node_modules client/mobile/node_modules",
    "mock": "esky-mock -c mock/config.js",
    "test": "NODE_ENV=test mocha",
    "site-init": "cd sites && npm run init",
    "site-start": "cd sites && npm run start",
    "site-build": "cd sites && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fegos/official-site.git"
  },
  "engines": {
    "node": ">=8.6"
  }
}